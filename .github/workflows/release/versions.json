{
  ".pubnub.yml": [
    { "pattern": "^version: \"(.+)\"$", "cleared": true }
  ],
  "package.json": [
    { "pattern": "^\\s{2,}\"version\": \"(.+)\",$", "cleared": true }
  ],
  "src/core/components/config.js": [
    { "pattern": "^\\s{2,}return '((\\d+\\.?){2,})';$", "cleared": true }
  ],
  "README.md": [
    { "pattern": "javascript/pubnub.((\\d+\\.?){2,}).js$", "cleared": true },
    { "pattern": "javascript/pubnub.((\\d+\\.?){2,}).min.js$", "cleared": true }
  ]
}